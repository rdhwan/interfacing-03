generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Temperature {
  id         Int      @id @default(autoincrement())
  sensorA    Float
  sensorB    Float
  sensorC    Float
  sensorAVG  Float
  controller Float
  timestamp  DateTime @default(now())
}
